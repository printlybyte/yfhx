<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="IncDecView">
        <attr name="inc_icon" format="reference"/>
        <attr name="dec_icon" format="reference"/>
        <attr name="view_background" format="reference|color"/>
        <attr name="inc_dec_button_color" format="reference|color"/>
        <attr name="border_width" format="reference|dimension"/>
        <attr name="border_color" format="reference|color"/>
        <attr name="startCounterValue" format="reference|string"/>
        <attr name="counterValueColor" format="reference|color"/>
    </declare-styleable>

    <declare-styleable name="SmoothCheckBox">
        <attr name="duration" format="integer"/>
        <attr name="stroke_width" format="dimension"/>
        <attr name="color_tick" format="color"/>
        <attr name="color_checked" format="color"/>
        <attr name="color_unchecked" format="color"/>
        <attr name="color_unchecked_stroke" format="color"/>
    </declare-styleable>
</resources>